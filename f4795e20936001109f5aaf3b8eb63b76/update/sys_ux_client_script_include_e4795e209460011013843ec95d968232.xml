        <script><![CDATA[function include(...args) {
    const userPrefs = args[1];

    /**
     * getUserPreference method returns user preference value.
     *
     * @param name
     * @return {boolean}
     */
    const getUserPreference = (name) => {
        return userPrefs[name] === 'true' || userPrefs[name] === true;
    };

    return ({
        getUserPreference: getUserPreference
    });
}]]></script>
